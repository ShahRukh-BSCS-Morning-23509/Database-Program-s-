CREATE TABLE Students (
    StudentID NUMBER PRIMARY KEY,
    Name VARCHAR2(50),
    Grade VARCHAR2(10),
    Department VARCHAR2(50)
);
 
INSERT INTO Students (StudentID, Name, Grade, Department)
VALUES (1, 'Ali', 'A', 'Computer Science');

